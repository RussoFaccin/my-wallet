{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:vue/essential",
        "standard"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "space-before-function-paren": "off",
        "no-console": "warn",
        "semi": "off",
        "no-unused-vars": "warn",
        "lines-between-class-members": "off",
        "no-trailing-spaces": "off"
    }
}